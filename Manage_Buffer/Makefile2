all: exec clean

exec: task2
	./task2

task2: DemoTest2_2.o manage_buff.o buf_manager_statistic.o manage_strg.o dbHandler.o
	gcc DemoTest2_2.o manage_buff.o buf_manager_statistic.o manage_strg.o dbHandler.o -o task2

DemoTest2_2.o: DemoTest2_2.c help_test.h dbHandler.h manage_strg.h buf_manager_statistic.h manage_buff.h
	gcc -c DemoTest2_2.c

manage_buff.o: manage_buff.c define.h dbHandler.h manage_strg.h buf_manager_statistic.h manage_buff.h
	gcc -c manage_buff.c

buf_manager_statistic.o: buf_manager_statistic.c manage_buff.h buf_manager_statistic.h
	gcc -c buf_manager_statistic.c

manage_strg.o: manage_strg.c dbHandler.h manage_strg.h
	gcc -c manage_strg.c

dbHandler.o: dbHandler.c dbHandler.h
	gcc -c dbHandler.c

clean:
	rm -rf *.o exec